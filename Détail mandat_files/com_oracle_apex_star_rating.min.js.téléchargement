function com_oracle_apex_star_rating(e,b){var c=($v("pScreenReaderMode")==="YES"),d,a;b.callback=function(f,g){a.trigger("change")};b.focus=function(f,g){a.trigger("focusin",{value:f})};b.blur=function(f,g){a.trigger("focusout",{value:f})};a=apex.jQuery("#"+e);d=a.find("input[type=radio]");if(c){a.find("button").click(function(){$s(e,"")})}else{d=d.rating(b)}apex.widget.initPageItem(e,{setValue:function(f){if(c){if(f){d.filter("[value="+f+"]").attr("checked",true)}else{d.attr("checked",false)}}else{d.rating("select",String(f))}},getValue:function(){var f=d.filter(":checked");return(f.length===0)?"":f.val()},enable:function(){if(c){d.removeAttr("disabled")}else{d.rating("enable")}},disable:function(){$s(e,"");if(c){d.attr("disabled","disabled")}else{d.rating("disable")}}})};